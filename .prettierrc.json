{
  "semi": true,
  "singleQuote": true,
  "trailingComma": "es5",
  "printWidth": 100,
  "overrides": [
    {
      "files": ["*.ts", "*.tsx"],
      "options": {
        "tabWidth": 4
      }
    },
    {
      "files": ["*.js", "*.jsx"],
      "options": {
        "tabWidth": 2
      }
    }
  ]
}
